		"turbo": "latest"
	},
	"engines": {
		"npm": ">=7.0.0"
	},
	"packageManager": "npm@9.4.0",
	"dependencies": {
		"react-countup": "^6.4.1",
		"reveal-on-scroll-react": "^0.0.6"
	}
}
